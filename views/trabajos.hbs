<div class="text-center m-5">
    <hr>
    <h1>
        {{ titulo }}
    </h1>
    <hr>
</div>


<div class="container">
    <div class="text-center m-5">
        <h2>
            Lista de contactos
        </h2>
    </div>
    <table class="table table-dark table-hover">
        <thead>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Mensaje</th>
        </thead>
        <tbody>
            {{#each results}}
            <tr>
                <td>{{nombre}}</td>
                <td>{{apellido}}</td>
                <td>{{mensaje}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>